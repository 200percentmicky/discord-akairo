{
	"name": "akairo-test",
	"version": "0.0.0",
	"main": "dist/bot.js",
	"license": "MIT",
	"scripts": {
		"base_start": "node --enable-source-maps --trace-warnings dist/bot.js",
		"base_build": "yarn rimraf dist && mkdir dist",
		"test": "yarn build && yarn base_start",
		"start": "yarn build && yarn base_start start",
		"start:swc": "yarn build:swc && yarn base_start start",
		"build": "yarn base_build && yarn tsc",
		"build:swc": "yarn base_build && yarn cp-cli ./src/auth.json ./dist/auth.json && yarn swc src -d ./dist"
	},
	"dependencies": {
		"chalk": "4",
		"discord.js": "npm:@notenoughupdates/discord.js@dev",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.204",
		"@types/lodash": "^4.14.182",
		"@types/node": "^17.0.43",
		"cp-cli": "^2.0.0",
		"discord-api-types": "0.34.0"
	},
	"imports": {
		"#discord-akairo": {
			"default": "./dist/import.js"
		}
	},
	"private": true
}
