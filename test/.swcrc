{
	"$schema": "https://json.schemastore.org/swcrc",
	"sourceMaps": true,
	"module": {
		"type": "commonjs",
		"strictMode": true,
		"noInterop": false
	},
	"jsc": {
		"externalHelpers": false,
		"target": "es2022",
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"decorators": false,
			"dynamicImport": true
		},
		"transform": {
			"legacyDecorator": true,
			"decoratorMetadata": false,
			"react": {
				"throwIfNamespace": false,
				"development": false,
				"useBuiltins": false,
				"pragma": "React.createElement",
				"pragmaFrag": "React.Fragment",
				"importSource": "react"
			}
		},
		"keepClassNames": true
	},
	"exclude": ["node_modules", "dist"]
}
